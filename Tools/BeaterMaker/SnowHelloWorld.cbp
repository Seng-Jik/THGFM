<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="BeaterMaker" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/SnowHelloWorld" prefix_auto="1" extension_auto="1" />
				<Option object_output="bin/Debug/obj/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library="lib/libSnow_Dbg.a" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/SnowHelloWorld" prefix_auto="1" extension_auto="1" />
				<Option object_output="bin/Release/obj/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="lib/libSnow.a" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add directory="$(#sdl2.include)/SDL2" />
			<Add directory="include" />
		</Compiler>
		<Linker>
			<Add library="mingw32" />
			<Add library="SDL2main" />
			<Add library="SDL2_image" />
			<Add library="SDL2.dll" />
			<Add library="user32" />
			<Add library="gdi32" />
			<Add library="winmm" />
			<Add library="dxguid" />
			<Add library="SDL2_Mixer" />
			<Add directory="$(#sdl2.lib)" />
			<Add directory="lib" />
		</Linker>
		<ExtraCommands>
			<Add after="XCOPY $(#sdl2)\bin\*.dll $(TARGET_OUTPUT_DIR) /D /Y" />
		</ExtraCommands>
		<Unit filename="main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
